{
	"lspconfig": {
		"lua_ls": {
			"Lua": {
				"diagnostics.disable": [
					"missing-fields"
				],
				"runtime": {
					"version": "LuaJIT",
					"special": {
						"spec": "require"
					}
				},
				"workspace": {
					"checkThirdParty": false,
					"library": {
						"$VIMRUNTIME/lua": true,
						"$HOME/.config/nvim/lua": true
					}
				}
			}
		}
	}
}
